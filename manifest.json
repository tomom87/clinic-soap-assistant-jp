{
  "manifest_version": 3,
  "name": "AI Medical Scribe",
  "version": "1.0.0",
  "description": "Medical AI Scribe using Gemini 2.5 Flash for SOAP note generation.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}